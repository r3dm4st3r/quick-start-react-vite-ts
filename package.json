    "formik": "^2.2.9",
    "lodash.clonedeep": "^4.5.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-haiku": "^2.0.3",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.0",
