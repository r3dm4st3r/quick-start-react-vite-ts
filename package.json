  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "code:verify": "eslint . --ext .ts,.tsx,.js,.jsx --ignore-pattern node_modules/",
    "code:fix": "eslint --fix . --ext .ts,.tsx,.js,.jsx --ignore-pattern node_modules/",
    "code:check": "prettier --check .",
    "code:format": "prettier --write ."
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
